{
  "name": "eventbuddy",
  "version": "2.0.0",
  "description": "EventBuddy - Phase 1: Charity Member & Events Management",
  "main": "dist/main.js",
  "scripts": {
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "test": "jest",
    "lint": "eslint src/",
    "build": "nest build",
    "clean": "lsof -ti:3001 | xargs kill -9 2>/dev/null || true",
    "start": "npm run clean && npm run build && pm2 restart eventbuddy || pm2 start dist/main.js --name eventbuddy",
    "stop": "pm2 stop eventbuddy || true",
    "restart": "npm run start",
    "docker:build": "docker build -t eventbuddy .",
    "docker:run": "docker run -p 3000:3000 eventbuddy",
    "db:generate": "prisma generate",
    "db:setup": "docker-compose exec postgres psql -U eventbuddy_user -d eventbuddy -c \"CREATE EXTENSION IF NOT EXISTS citext;\" && npm run db:push -- --accept-data-loss",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/seed.ts",
    "db:reset": "prisma migrate reset --force && npm run db:setup",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "charity",
    "events",
    "member-management",
    "nodejs",
    "prisma",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^5.7.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/node": "^20.10.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "eslint": "^8.45.0",
    "eslint-config-standard": "^17.1.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.2",
    "typescript": "^5.9.2"
  }
}
