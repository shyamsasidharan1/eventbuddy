{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": ["<rootDir>/setup.ts"],
  "testTimeout": 30000,
  "maxWorkers": 1,
  "collectCoverageFrom": [
    "src/**/*.(ts|js)",
    "!src/**/*.d.ts",
    "!src/main.ts"
  ]
}